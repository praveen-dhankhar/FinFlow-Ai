{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:performance": "jest --testPathPattern=performance",
    "test:all": "npm run test && npm run test:e2e && npm run test:performance",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "lhci autorun --upload.target=temporary-public-storage",
    "analyze": "npm run build && npx @next/bundle-analyzer",
    "test:setup": "playwright install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy:staging": "vercel --target staging",
    "deploy:production": "vercel --prod",
    "deploy:preview": "vercel",
    "analyze:build": "ANALYZE=true npm run build",
    "type-check": "tsc --noEmit",
    "format:check": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-query-devtools": "^5.87.4",
    "@types/js-cookie": "^3.0.6",
    "axios": "^1.12.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.2.0",
    "framer-motion": "^11.18.2",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.460.0",
    "next": "^13.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "react-use-gesture": "^9.1.3",
    "recharts": "^2.15.4",
    "tailwind-merge": "^2.6.0",
    "use-debounce": "^10.0.6",
    "zod": "^3.25.76",
    "zustand": "^4.5.7",
    "@sentry/nextjs": "^8.0.0",
    "@sentry/profiling-node": "^8.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@playwright/test": "^1.35.0",
    "@next/bundle-analyzer": "^13.5.6",
    "@lhci/cli": "^0.12.0",
    "@storybook/addon-a11y": "^7.6.0",
    "@storybook/addon-backgrounds": "^7.6.0",
    "@storybook/addon-controls": "^7.6.0",
    "@storybook/addon-docs": "^7.6.0",
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/addon-interactions": "^7.6.0",
    "@storybook/addon-links": "^7.6.0",
    "@storybook/addon-themes": "^7.6.0",
    "@storybook/addon-viewport": "^7.6.0",
    "@storybook/nextjs": "^7.6.0",
    "@storybook/react": "^7.6.0",
    "storybook": "^7.6.0",
    "eslint": "^8",
    "eslint-config-next": "13.5.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^1.3.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=16.20.0"
  }
}
